<!DOCTYPE html>
<html>
  <head>
  	<!-- {{ title }} tells HJS to use variable title when
  		rendering this page -->
    <title>{{ title }}</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
  	<h1>{{ title }}</h1>

  	<!-- This can be turned into a menu easily -->
    <div>
    <h1>Current List</h1>
     <ul>
     {{#doc_list}}   <!-- Iterate over the array in users variable -->
      <li>{{title}} {{customer}}</li>  <!-- fields in the array entry -->
     {{/doc_list}}	<!-- end of loop -->
    </ul>
    </div>
    <p></p>
  </body>
</html>
